<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	id="WebApp_ID" version="3.1">
	<display-name>class-example</display-name>
	<welcome-file-list>
		<welcome-file>add.html</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>index</servlet-name>
		<jsp-file>add.html</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>index</servlet-name>
		<url-pattern></url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>AddServlet</servlet-name>
		<servlet-class>it.polimi.classexample.servlets.AddServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AddServlet</servlet-name>
		<url-pattern>/add</url-pattern>
	</servlet-mapping>
		<servlet>
		<servlet-name>AddJsp</servlet-name>
		<jsp-file>/add.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>AddJsp</servlet-name>
		<url-pattern>/addJsp</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>StatefulAddJsp</servlet-name>
		<jsp-file>/statefulAdd.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>StatefulAddJsp</servlet-name>
		<url-pattern>/statefulAdd</url-pattern>
	</servlet-mapping>
</web-app>